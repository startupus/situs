# API Server Configuration - Development
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/situs_dev?schema=public"
# JWT_SECRET - УДАЛЕН из .env по соображениям безопасности
# Используйте переменную окружения: export JWT_SECRET=$(openssl rand -hex 64)
# Для разработки используется fallback в environment.ts
JWT_EXPIRES_IN="7d"
NODE_ENV="development"
PORT=3001
FRONTEND_URL="http://localhost:3000"
CORS_ORIGINS="http://localhost:3000,http://localhost:5173"
BCRYPT_SALT_ROUNDS=10
LOG_LEVEL="debug"
DEV_MODE=true
SEED_DATABASE=true
ENABLE_SWAGGER=true
ENABLE_CORS=true

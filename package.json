{
  "name": "situs-visual-website-builder",
  "version": "1.0.0",
  "description": "Специализированный визуальный редактор для создания веб-сайтов с drag-and-drop интерфейсом",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "node -e \"if (!require('fs').existsSync('src')) { console.error('❌ Запустите проект из корневой папки Situs/\\nТекущая директория:', process.cwd()); process.exit(1); }\"",
    "dev": "vite",
    "dev:situs": "vite --port 5177",
    "dev:redaktus": "vite --port 5178",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:storybook": "test-storybook",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-components": "node scripts/check-components.cjs",
    "create-component": "node scripts/create-component.cjs",
    "migrate-components": "node scripts/migrate-components.cjs",
    "bulk-migrate": "node scripts/bulk-migrate-tailgrids.cjs",
    "test-components": "npm run check-components && npm run test",
    "browser-tools:start": "tsx scripts/mcp-browser-tools.ts start",
    "browser-tools:stop": "tsx scripts/mcp-browser-tools.ts stop",
    "browser-tools:status": "tsx scripts/mcp-browser-tools.ts status",
    "browser-tools:ready": "tsx scripts/mcp-browser-tools.ts ready",
    "browser-tools:manager:start": "tsx scripts/browser-tools-manager.ts start",
    "browser-tools:manager:stop": "tsx scripts/browser-tools-manager.ts stop",
    "browser-tools:manager:status": "tsx scripts/browser-tools-manager.ts status",
    "browser-tools:manager:health": "tsx scripts/browser-tools-manager.ts health",
    "browser-tools:cleanup": "pkill -f browser-tools-server && pkill -f browser-tools-mcp",
    "browser-tools:fix": "node scripts/browser-tools-fix.cjs",
    "browser-tools:test": "node scripts/browser-tools-fix.cjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.0.18",
    "@monaco-editor/react": "^4.6.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.5",
    "@radix-ui/react-slot": "^1.1.5",
    "@radix-ui/react-switch": "^1.1.5",
    "@radix-ui/react-tabs": "^1.1.5",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "apexcharts": "^5.3.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "email-validator": "^2.0.4",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "i18next": "^25.3.2",
    "jsonp": "^0.2.1",
    "jsonwebtoken": "^9.0.2",
    "jsvectormap": "^1.7.0",
    "lucide-react": "^0.469.0",
    "morgan": "^1.10.1",
    "pigeon-maps": "^0.22.1",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@playwright/test": "^1.41.0",
    "@storybook/addon-essentials": "^8.5.4",
    "@storybook/addon-interactions": "^8.5.4",
    "@storybook/addon-links": "^8.5.4",
    "@storybook/addon-onboarding": "^8.5.4",
    "@storybook/blocks": "^8.5.4",
    "@storybook/react": "^8.5.4",
    "@storybook/react-vite": "^8.5.4",
    "@storybook/test": "^8.5.4",
    "@storybook/test-runner": "^0.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonp": "^0.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.10.6",
    "@types/prismjs": "^1.26.5",
    "@types/react-slick": "^0.23.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "storybook": "^8.5.4",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "^6.0.8",
    "vitest": "^2.1.8"
  }
}

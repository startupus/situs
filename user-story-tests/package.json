{
  "name": "habus-comprehensive-testing",
  "version": "1.0.0",
  "description": "Интеллектуальная система комплексного тестирования экосистемы Хабус",
  "main": "run-comprehensive-testing.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:auto": "ts-node run-comprehensive-testing.ts",
    "test:auth": "ts-node run-comprehensive-testing.ts --category auth",
    "test:billing": "ts-node run-comprehensive-testing.ts --category billing",
    "test:ai": "ts-node run-comprehensive-testing.ts --category ai",
    "test:referral": "ts-node run-comprehensive-testing.ts --category referral",
    "test:report": "ts-node run-comprehensive-testing.ts --report-only",
    "test:interactive": "ts-node run-comprehensive-testing.ts --interactive"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "keywords": [
    "testing",
    "automation",
    "ai",
    "microservices",
    "coverage",
    "intelligent"
  ],
  "author": "Startupus Team",
  "license": "MIT"
}